<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <data>
        <import type="android.view.View"/>
        <variable
            name="TrangChuViewModel"
            type="vn.nhattamsoft.project_mau.viewmodels.TrangChuViewModel" />
    </data>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".fragments.TrangChuFragment"
        android:id="@+id/layoutcha"
        >
        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <LinearLayout
                    android:id="@+id/ln"
                    android:layout_width="match_parent"
                    android:layout_height="225dp"
                    android:background="@drawable/radiusbottom"
                    android:orientation="vertical">

                </LinearLayout>
                <androidx.cardview.widget.CardView
                    android:id="@+id/tt"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="40dp"
                    app:cardCornerRadius="8dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginBottom="10dp"
                    app:cardBackgroundColor="@color/white"
                    app:cardElevation="2dp"
                    >
                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="left"
                        android:layout_margin="10dp"
                        android:padding="5dp"

                        >

                        <de.hdodenhof.circleimageview.CircleImageView
                            android:id="@+id/btnDraw"
                            android:layout_width="24dp"
                            android:layout_height="24dp"
                            android:layout_alignParentStart="true"
                            android:layout_centerInParent="true"
                            android:layout_marginStart="0dp"
                            android:background="@drawable/menu"
                            android:padding="@dimen/padd_20"
                            android:foreground="?android:attr/selectableItemBackground"
                            />
                        <LinearLayout
                            android:id="@+id/lin"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_toEndOf="@+id/btnDraw"
                            android:orientation="vertical"
                            android:layout_marginLeft="15dp"
                            >

                            <TextView
                                android:id="@+id/title"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="Xin chào!"
                                android:textColor="@color/black"/>

                            <TextView
                                android:id="@+id/body"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="Nguyễn Hoàng Vịnh"
                                android:textColor="@color/primary"
                                android:textSize="17sp"
                                android:textStyle="bold" />

                        </LinearLayout>
                        <ImageButton
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:background="@drawable/notification"
                            android:layout_alignParentEnd="true"
                            android:layout_centerInParent="true"
                            android:backgroundTint="@color/gray"
                            />
                    </RelativeLayout>
                </androidx.cardview.widget.CardView>
                <androidx.cardview.widget.CardView
                    android:id="@+id/cardBieuDo"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:cardCornerRadius="8dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginBottom="10dp"
                    app:cardElevation="2dp"
                    app:cardBackgroundColor="@color/white"
                    android:layout_below="@+id/tt"
                    >
                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:paddingLeft="17dp"
                        android:paddingTop="12dp"
                        >
                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:id="@+id/rltieude"
                            android:paddingRight="17dp">

                            <TextView
                                android:id="@+id/tieudetk"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentStart="true"
                                android:layout_centerInParent="true"
                                android:text="Số người có việc làm"
                                android:textColor="@color/black"
                                android:textSize="15sp"
                                android:textStyle="bold" />

                            <TextView
                                android:id="@+id/labelTenTinh"
                                android:layout_width="130dp"
                                android:layout_height="wrap_content"
                                android:layout_alignEnd="@+id/tieudetk"
                                android:layout_alignParentEnd="true"
                                android:layout_centerInParent="true"
                                android:layout_marginEnd="15dp"
                                android:textAlignment="textEnd"
                                android:textColor="@color/black" />
                        </RelativeLayout>
                        <RelativeLayout
                            android:id="@+id/bieudo"
                            android:layout_width="150dp"
                            android:layout_height="110dp"
                            android:layout_below="@+id/rltieude"
                            android:layout_centerHorizontal="true"
                            android:layout_alignParentStart="true"
                            android:layout_above="@+id/view2"
                            >
                            <com.github.mikephil.charting.charts.PieChart
                                android:id="@+id/chart2"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                />
                        </RelativeLayout>
                        <RelativeLayout
                            android:id="@+id/tk"
                            android:layout_width="wrap_content"
                            android:layout_height="110dp"
                            android:layout_below="@+id/rltieude"
                            android:layout_alignParentEnd="true"
                            android:layout_toEndOf="@+id/bieudo"
                            android:layout_marginTop="10dp"
                            android:paddingBottom="12dp"
                            android:paddingRight="17dp"
                            >
                            <TextView
                                android:id="@+id/trongdo"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Trong đó:"
                                android:textColor="@color/black"
                                android:layout_marginLeft="5dp"
                                android:layout_marginTop="5dp"
                                android:layout_marginBottom="5dp"/>
                            <RelativeLayout
                                android:id="@+id/tknam"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_below="@id/trongdo"
                                android:layout_marginStart="13dp"
                                >
                                <ImageView
                                    android:id="@+id/circlenam"
                                    android:layout_width="7sp"
                                    android:layout_height="7sp"
                                    android:background="@drawable/circle"
                                    android:backgroundTint="#0799ed"
                                    android:layout_centerVertical="true"
                                    />
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text=" Nam"
                                    android:layout_toEndOf="@+id/circlenam"
                                    android:layout_centerVertical="true"
                                    />
                                <TextView
                                    android:id="@+id/TongNam"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="..."
                                    android:textSize="15sp"
                                    android:textColor="#0799ed"
                                    android:layout_alignParentEnd="true"
                                    android:layout_centerVertical="true"
                                    android:layout_marginEnd="15dp"
                                    />
                            </RelativeLayout>
                            <RelativeLayout
                                android:id="@+id/tknu"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_below="@id/tknam"
                                android:layout_marginStart="13dp"
                                >
                                <ImageView
                                    android:id="@+id/circlenu"
                                    android:layout_width="7sp"
                                    android:layout_height="7sp"
                                    android:background="@drawable/circle"
                                    android:backgroundTint="#e6395c"
                                    android:layout_centerVertical="true"
                                    />
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text=" Nữ"
                                    android:layout_toEndOf="@+id/circlenu"
                                    android:layout_centerVertical="true"
                                    />

                                <TextView
                                    android:id="@+id/TongNu"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_alignParentEnd="true"
                                    android:layout_marginEnd="15dp"
                                    android:text="..."
                                    android:textColor="#e6395c"
                                    android:textSize="15sp" />
                            </RelativeLayout>
                            <View
                                android:id="@+id/viewTong"
                                android:layout_width="match_parent"
                                android:layout_height="1dp"
                                android:layout_below="@+id/tknu"
                                android:layout_centerInParent="true"
                                android:layout_marginTop="4dp"
                                android:layout_marginBottom="4dp"
                                android:layout_marginStart="13dp"
                                android:layout_marginEnd="13dp"
                                android:background="@color/gray2" />
                            <RelativeLayout
                                android:id="@+id/tong"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_below="@id/viewTong"
                                android:layout_marginStart="13dp"
                                >

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text=" Tổng:"
                                    android:textColor="@color/black"
                                    android:layout_centerVertical="true"
                                    />

                                <TextView
                                    android:id="@+id/TongNamNu"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_alignParentEnd="true"
                                    android:layout_marginEnd="15dp"
                                    android:text="..."
                                    android:textColor="@color/black"
                                    android:textSize="15sp" />
                            </RelativeLayout>
                        </RelativeLayout>

                        <View
                            android:id="@+id/view2"
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:layout_below="@+id/tk"
                            android:layout_centerInParent="true"
                            android:layout_marginTop="8dp"
                            android:background="@color/gray2" />
                        <Button
                            android:layout_width="wrap_content"
                            android:layout_height="40dp"
                            android:text="Xem chi tiết"
                            android:textColor="@color/primary"
                            android:layout_below="@id/view2"
                            android:textSize="14sp"
                            android:layout_alignParentEnd="true"
                            style="?android:attr/borderlessButtonStyle"
                            android:drawableRight="@drawable/ic_expansion_header_indicator_grey_24dp"
                            android:textAllCaps="false"
                            android:drawableTint="@color/primary"
                            />

                    </RelativeLayout>
                </androidx.cardview.widget.CardView>
                <LinearLayout
                    android:id="@+id/ln3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/cardBieuDo"
                    android:orientation="horizontal"
                    android:layout_marginEnd="@dimen/padd_15"
                    android:layout_marginStart="@dimen/padd_15"
                    android:layout_marginBottom="@dimen/padd_10"
                    >
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Số cung - cầu lao động"
                        android:textColor="@color/black"
                        android:textSize="15sp"
                        android:textStyle="bold"
                        />
                    <com.google.android.material.textfield.TextInputLayout
                        style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.ExposedDropdownMenu"
                        android:layout_width="match_parent"
                        android:layout_height="30dp"
                        android:gravity="right">
                        <AutoCompleteTextView
                            android:id="@+id/tkloai"
                            android:layout_width="150dp"
                            android:layout_height="wrap_content"
                            android:padding="0dp"/>
                    </com.google.android.material.textfield.TextInputLayout>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/ln1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/ln3"
                    android:orientation="horizontal"
                    >

                    <androidx.cardview.widget.CardView
                        android:id="@+id/tren15"
                        android:layout_width="wrap_content"
                        android:layout_height="70dp"
                        app:cardCornerRadius="5dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="5dp"
                        android:layout_marginBottom="10dp"
                        android:layout_weight="1"
                        app:cardBackgroundColor="#d9edff"
                        >
                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">
                            <FrameLayout
                                android:id="@+id/frame1"
                                android:layout_width="40dp"
                                android:layout_height="40dp"
                                android:background="@drawable/radius"
                                android:backgroundTint="#3C95E8"
                                android:layout_centerVertical="true"
                                android:layout_marginStart="10dp"
                                >
                                <ImageView
                                    android:layout_width="22dp"
                                    android:layout_height="22dp"
                                    android:background="@drawable/person"
                                    android:backgroundTint="@color/white"
                                    android:layout_gravity="center"/>
                            </FrameLayout>
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                android:layout_toEndOf="@+id/frame1"
                                android:layout_centerVertical="true">

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:orientation="horizontal">

                                    <TextView
                                        android:id="@+id/tkTren15"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="5dp"
                                        android:textColor="#0274db"
                                        android:textSize="18sp"
                                        android:text="..."
                                        />

                                </LinearLayout>
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="5dp"
                                    android:text="Trên 15 tuổi"
                                    android:textColor="@color/black"
                                    android:textSize="14sp"

                                    />
                            </LinearLayout>
                        </RelativeLayout>
                    </androidx.cardview.widget.CardView>
                    <androidx.cardview.widget.CardView
                        android:id="@+id/tgkt"
                        android:layout_width="wrap_content"
                        android:layout_height="70dp"
                        app:cardCornerRadius="5dp"
                        android:layout_marginLeft="5dp"
                        android:layout_marginRight="10dp"
                        android:layout_marginBottom="10dp"
                        android:layout_weight="1"
                        app:cardBackgroundColor="#fff5cc"
                        >
                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">
                            <FrameLayout
                                android:id="@id/frame2"
                                android:layout_width="40dp"
                                android:layout_height="40dp"
                                android:background="@drawable/radius"
                                android:backgroundTint="#eb9205"
                                android:layout_centerVertical="true"
                                android:layout_marginStart="10dp"
                                >
                                <ImageView
                                    android:layout_width="22dp"
                                    android:layout_height="22dp"
                                    android:background="@drawable/person"
                                    android:backgroundTint="@color/white"
                                    android:layout_gravity="center"/>

                            </FrameLayout>
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                android:layout_toEndOf="@+id/frame2"
                                android:layout_centerVertical="true">

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:orientation="horizontal">

                                    <TextView
                                        android:id="@+id/tkTGKT"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="5dp"
                                        android:textColor="#eb9205"
                                        android:textSize="18sp"
                                        android:text="..."
                                        />

                                </LinearLayout>
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="5dp"
                                    android:text="Tham gia kinh tế"
                                    android:textColor="@color/black"
                                    android:textSize="14sp"

                                    />
                            </LinearLayout>
                        </RelativeLayout>
                    </androidx.cardview.widget.CardView>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/ln2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/ln1"
                    android:orientation="horizontal"

                    >

                    <androidx.cardview.widget.CardView
                        android:id="@+id/ktgkt"
                        android:layout_width="wrap_content"
                        android:layout_height="70dp"
                        app:cardCornerRadius="5dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="5dp"
                        android:layout_marginBottom="10dp"
                        android:layout_weight="1"
                        app:cardBackgroundColor="#f0e6ff"
                        >
                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">
                            <FrameLayout
                                android:id="@id/frame3"
                                android:layout_width="40dp"
                                android:layout_height="40dp"
                                android:background="@drawable/radius"
                                android:backgroundTint="#7900eb"
                                android:layout_centerVertical="true"
                                android:layout_marginStart="10dp"
                                >
                                <ImageView
                                    android:layout_width="22dp"
                                    android:layout_height="22dp"
                                    android:background="@drawable/person"
                                    android:backgroundTint="@color/white"
                                    android:layout_gravity="center"/>

                            </FrameLayout>
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                android:layout_toEndOf="@+id/frame3"
                                android:layout_centerVertical="true">

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:orientation="horizontal">

                                    <TextView
                                        android:id="@+id/tkKTGKT"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="5dp"
                                        android:textColor="#7900eb"
                                        android:text="..."
                                        android:textSize="18sp" />

                                </LinearLayout>
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="5dp"
                                    android:text="Không tham gia kinh tế"
                                    android:textColor="@color/black"
                                    android:textSize="14sp"

                                    />
                            </LinearLayout>
                        </RelativeLayout>
                    </androidx.cardview.widget.CardView>
                    <androidx.cardview.widget.CardView
                        android:id="@+id/dqdt"
                        android:layout_width="wrap_content"
                        android:layout_height="70dp"
                        app:cardCornerRadius="5dp"
                        android:layout_marginLeft="5dp"
                        android:layout_marginRight="10dp"
                        android:layout_marginBottom="10dp"
                        android:layout_weight="1"
                        app:cardBackgroundColor="#a6ffd2"
                        >
                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">
                            <FrameLayout
                                android:id="@+id/frame4"
                                android:layout_width="40dp"
                                android:layout_height="40dp"
                                android:background="@drawable/radius"
                                android:backgroundTint="#04db53"
                                android:layout_centerVertical="true"
                                android:layout_marginStart="10dp"
                                >
                                <ImageView
                                    android:layout_width="22dp"
                                    android:layout_height="22dp"
                                    android:background="@drawable/person"
                                    android:backgroundTint="@color/white"
                                    android:layout_gravity="center"/>

                            </FrameLayout>
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                android:layout_toEndOf="@+id/frame4"
                                android:layout_centerVertical="true">

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:orientation="horizontal">

                                    <TextView
                                        android:id="@+id/tkDQDT"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="5dp"
                                        android:text="..."
                                        android:textColor="#04db53"
                                        android:textSize="18sp" />

                                </LinearLayout>
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="5dp"
                                    android:text="Đã qua đào tạo"
                                    android:textColor="@color/black"
                                    android:textSize="14sp"
                                    />
                            </LinearLayout>
                        </RelativeLayout>
                    </androidx.cardview.widget.CardView>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginBottom="20dp"
                    android:layout_marginTop="10dp"
                    android:layout_below="@+id/ln2"
                    >
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Tiện ích"
                        android:textSize="18sp"
                        android:textStyle="bold"
                        android:textColor="@color/black"
                        android:layout_marginBottom="@dimen/padd_10"
                        android:layout_marginLeft="10dp"
                        />
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:gravity="center_horizontal"
                        android:layout_marginBottom="7dp"

                        >
                        <LinearLayout
                            android:layout_width="125dp"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            >
                            <ImageButton
                                android:id="@+id/btnTraCuu"
                                android:layout_width="55dp"
                                android:layout_height="55dp"
                                android:layout_gravity="center_horizontal"
                                android:src="@drawable/employees2"
                                android:scaleType="fitXY"
                                android:background="@drawable/radius_imagebutton"
                                android:padding="12dp"
                                />
                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="Tra cứu cung\nlao động"
                                android:textSize="15sp"
                                android:textAlignment="center"
                                />
                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="125dp"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:layout_marginLeft="5dp"
                            android:layout_marginRight="5dp"

                            >
                            <ImageButton
                                android:id="@+id/btnMap"
                                android:layout_width="55dp"
                                android:layout_height="55dp"
                                android:layout_gravity="center_horizontal"
                                android:src="@drawable/map5"
                                android:scaleType="fitXY"
                                android:background="@drawable/radius_imagebutton"
                                android:padding="12dp"

                                />
                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="Bản đồ"
                                android:textSize="15sp"
                                android:textAlignment="center"
                                />
                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="125dp"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            >
                            <ImageButton
                                android:id="@+id/btnThongKe"
                                android:layout_width="55dp"
                                android:layout_height="55dp"
                                android:layout_gravity="center_horizontal"
                                android:src="@drawable/barchart"
                                android:scaleType="fitXY"
                                android:background="@drawable/radius_imagebutton"
                                android:padding="12dp"
                                />
                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="Thống kê"
                                android:textSize="15sp"
                                android:textAlignment="center"
                                />
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:gravity="center_horizontal"
                        android:layout_marginBottom="7dp"
                        >
                        <LinearLayout
                            android:layout_width="125dp"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">
                            <ImageButton
                                android:id="@+id/btnKeHoach"
                                android:layout_width="55dp"
                                android:layout_height="55dp"
                                android:layout_gravity="center_horizontal"
                                android:src="@drawable/search8"
                                android:scaleType="fitXY"
                                android:background="@drawable/radius_imagebutton"
